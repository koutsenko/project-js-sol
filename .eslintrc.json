{
  "env": {
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": ["error", 2, {
      "SwitchCase": 1
    }],
    "eol-last": "error",
    "no-console": "off",
    "no-multiple-empty-lines": ["error", {
      "max": 1
    }],
    "no-trailing-spaces": "error"
  }
}